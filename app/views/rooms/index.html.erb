<h1>Rooms</h1>
<table border="1" cellpadding="6">
  <tr>
    <th>Number</th>
    <th>Type</th>
    <th>Price</th>
    <th>Status</th>
    <th></th>
  </tr>
  <% @rooms.each do |room| %>
    <tr>
      <td><%= room.number %></td>
      <td><%= room.room_type %></td>
      <td>$ <%= room.price %></td>
      <td><%= room.status %></td>
      <td><%= link_to "Show", room_path(room) %></td>
    </tr>
  <% end %>
</table>
